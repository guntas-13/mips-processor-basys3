    %jmp/1 T_0.1, 6;
    %pushi/vec4 31, 31, 5;
    %store/vec4 v0x138720480_0, 0, 5;
    %jmp T_0.3;
T_0.0 ;
    %load/vec4 v0x138720350_0;
    %store/vec4 v0x138720480_0, 0, 5;
    %jmp T_0.3;
T_0.1 ;
