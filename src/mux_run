    .scope S_0x13871fef0;
T_0 ;
    %wait E_0x138720240;
    %load/vec4 v0x1387202a0_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 1;
    %cmp/u;
    %jmp/1 T_0.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 1;
